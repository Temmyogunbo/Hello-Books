{
  "name": "hellobooks",
  "version": "6.11.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/dist/bin/www.js",
    "build": "rm -rf server/dist && babel server/ --out-dir server/dist/ --copy-files",
    "start:dev": "babel-node -- server/bin/www.js",
    "dev": "npm run build:dev",
    "build:dev": "nodemon --watch server --exec babel-node -- ./server/bin/www.js test:watch",
    "pretest": "npm run cleardb && cross-env NODE_ENV=test sequelize db:migrate && cross-env NODE_ENV=test sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 10000 ./server/test/**/*.js",
    "test:watch": "npm run test -- --watch",
    "cleardb": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "heroku-postbuild": "cd client && npm i &&  npm run build && cd ../ && npm run build"
  },
  "engines": {
    "node": "6.11.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "server/test",
      "server/seeders",
      "server/config"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "cloudinary": "^1.9.0",
    "cookie-parser": "^1.4.3",
    "cron": "^1.3.0",
    "cross-env": "^5.1.1",        
    "css-loader": "^0.28.5",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "draft-js": "^0.10.1",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.1",
    "jade": "~1.11.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^7.4.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "morgan": "^1.8.2",
    "moxios": "^0.4.0",
    "node-sass": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "save-dev": "^2.0.0",
    "sendgrid": "^5.2.3",
    "sequelize": "^4.13.5",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "^2.4.3",
    "socket.io": "^2.0.4",
    "url-loader": "^0.5.9",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2"
  }
}
